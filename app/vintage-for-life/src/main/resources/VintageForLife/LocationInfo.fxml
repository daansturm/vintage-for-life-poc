<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="borderpane" style="-fx-border-color: lightgrey; -fx-border-width: 2; -fx-border-style: solid; -fx-border-radius: 1;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="VintageForLife.LocationInfo">
   <center>
      <AnchorPane>
         <children>
            <VBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <Label fx:id="route" text="Route:" />
                  <Label fx:id="route_info"/>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </center>
   <effect>
      <DropShadow blurType="GAUSSIAN" height="0.0" radius="0.0" width="0.0" />
   </effect>
</BorderPane>
