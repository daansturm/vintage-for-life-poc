<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<DialogPane maxHeight="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="VintageForLife.LeveringRetour">
    <header>
      <AnchorPane>
         <children>
            <VBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <Label fx:id="LeveringRetourtxt" text="Levering " textAlignment="CENTER">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </children>
      </AnchorPane>
    </header>
    <content>
      <AnchorPane>
         <children>
            <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <center>
                  <VBox BorderPane.alignment="CENTER">
                     <children>
                        <Label fx:id="plaatstxt" text="Plaats: " textAlignment="CENTER" />
                        <Label fx:id="adrestxt" text="Adres :  " textAlignment="CENTER" />
                        <Label fx:id="postcodetxt" text="postcode: " textAlignment="CENTER" />
                        <Label fx:id="landtxt" text="Land: " textAlignment="CENTER" />
                     </children>
                  </VBox>
               </center>
               <right>
                  <MenuButton fx:id="change_route" mnemonicParsing="false" BorderPane.alignment="CENTER">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Action 1" />
                      <MenuItem mnemonicParsing="false" text="Action 2" />
                    </items>
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@Pictures/change_icon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </MenuButton>
               </right>
            </BorderPane>
         </children></AnchorPane>
    </content>
    <expandableContent>
      <AnchorPane maxHeight="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
            <Label layoutX="14.0" layoutY="6.0" text="Bestellingen " textAlignment="CENTER" />
        </AnchorPane>
    </expandableContent>
   <effect>
      <DropShadow />
   </effect>
</DialogPane>
